
// Following examples indicate different ways of writing the fib sequence

function fibonacci (x) {
    if [<= x 2] 1 [+ [fibonacci [- x 1]] [fibonacci [- x 2]]]
}

function fibonacci2 (x) {
    if [> x 2] [+ [fibonacci2 [- x 1]] [fibonacci2 [- x 2]]] {
        1
    }
}

function fibonacci3 (x) {
    if [> x 2] {
        + [fibonacci3 [- x 1]] [fibonacci3 [- x 2]]
    } {
        1
    }
}

println "** FIBONACCI EXAMPLE**"
println "The fibonacci answer to 12 is(144) " [fibonacci 12]
